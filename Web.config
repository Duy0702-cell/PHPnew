<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="AccessDb"
         connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=|DataDirectory|\Shop.mdb;Persist Security Info=False;"
         providerName="System.Data.OleDb" />
  </connectionStrings>

  <appSettings>
    <add key="AdminUser" value="admin" />
    <add key="AdminPass" value="123456" />
  </appSettings>

  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <sessionState timeout="30" />
  </system.web>
</configuration>
